{
  "version": 2,
  "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node",
        "config": {
        "functions": {
          "api/*.js": { "maxDuration": 300 },
        
        }
      }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ]
}
